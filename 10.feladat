#első verzió
def oszto(szam):
    for i in range(1,21):
        if szam%i!=0:
            return False
    return True
szam=1
while True:#amíg nem találunk olyan számot ami osztható 1-től 20-ig mindennel addig adjunk hozzá egyet
    if oszto(szam):
        break
    szam+=1
print(szam)


#hamarabbi futás:
def oszto(szam):
    for i in range(1,21):
        if szam%i!=0:
            return False
    return True
szam=20
while True:
    if oszto(szam):
        break
    szam+=20
print(szam)
